
CREATE TABLE client (
    creation_date timestamp default current_timestamp,
    update_date timestamp default current_timestamp,
    id text UNIQUE NOT NULL PRIMARY KEY,
    clinician_id text NOT NULL,
    first_name text NOT NULL,
    last_name text NOT NULL,
    email_address text NOT NULL,
    birth_date date NOT NULL,
    sex text,
    postcode text,
    marital_status text,
    sexuality text,
    ethnicity text,
    indigenous boolean,
    children float,
    workforce_status text,
    education text
);


CREATE TABLE scale (
    creation_date timestamp default current_timestamp,
    update_date timestamp default current_timestamp,
    id text UNIQUE NOT NULL PRIMARY KEY,
    clinician_id text NOT NULL, 
    client_id text NOT NULL REFERENCES client,
    measure text NOT NULL,
    subscale text NOT NULL,
    date date NOT NULL,
    score float NOT NULL,
    pts float NOT NULL,
    se float NOT NULL,
    ci float NOT NULL,
    ci_upper float NOT NULL,
    ci_lower float NOT NULL,
    mean float NOT NULL,
    mean_reference text NOT NULL,
    sd float NOT NULL,
    sd_reference text NOT NULL,
    reliability float NOT NULL,
    reliability_reference text NOT NULL,
    confidence float NOT NULL,
    method text NOT NULL,
    population text NOT NULL,
    cutoff_label_1 text NOT NULL,
    cutoff_label_2 text NOT NULL,
    cutoff_label_3 text NOT NULL,
    cutoff_label_4 text NOT NULL,
    cutoff_label_5 text NOT NULL,
    cutoff_label_6 text NOT NULL,
    cutoff_value_1 float NOT NULL,
    cutoff_value_2 float NOT NULL,
    cutoff_value_3 float NOT NULL,
    cutoff_value_4 float NOT NULL,
    cutoff_value_5 float NOT NULL,
    cutoff_value_6 float NOT NULL,
    cutoff_reference_1 text NOT NULL,
    cutoff_reference_2 text NOT NULL,
    cutoff_reference_3 text NOT NULL,
    cutoff_reference_4 text NOT NULL,
    cutoff_reference_5 text NOT NULL,
    cutoff_reference_6 text NOT NULL
);



CREATE TABLE holding (
    creation_date timestamp default current_timestamp,
    id text UNIQUE NOT NULL PRIMARY KEY,
    clinician_id text NOT NULL,
    client_id text NOT NULL REFERENCES client,
    measure text NOT NULL,
    subscale text NOT NULL,
    mean float NOT NULL,
    mean_reference text NOT NULL,
    sd float NOT NULL,
    sd_reference text NOT NULL,
    reliability float NOT NULL,
    reliability_reference text NOT NULL,
    confidence float NOT NULL,
    method text NOT NULL,
    population text NOT NULL,
    cutoff_label_1 text NOT NULL,
    cutoff_label_2 text NOT NULL,
    cutoff_label_3 text NOT NULL,
    cutoff_label_4 text NOT NULL,
    cutoff_label_5 text NOT NULL,
    cutoff_label_6 text NOT NULL,
    cutoff_value_1 float NOT NULL,
    cutoff_value_2 float NOT NULL,
    cutoff_value_3 float NOT NULL,
    cutoff_value_4 float NOT NULL,
    cutoff_value_5 float NOT NULL,
    cutoff_value_6 float NOT NULL,
    cutoff_reference_1 text NOT NULL,
    cutoff_reference_2 text NOT NULL,
    cutoff_reference_3 text NOT NULL,
    cutoff_reference_4 text NOT NULL,
    cutoff_reference_5 text NOT NULL,
    cutoff_reference_6 text NOT NULL
);


