CREATE TABLE posttherapy_analytics (
    creation_date timestamp default current_timestamp,
    id text UNIQUE NOT NULL PRIMARY KEY,
    clinician_id text NOT NULL,
    client_id text NOT NULL REFERENCES client,
    principal_diagnosis text,
    secondary_diagnosis text,
    attendance_schedule text,
    cancellations float,
    non_attendances float,
    attendances float,
    premature_dropout text,
    therapy text,
    funding text,
    private_health_fund text,
    referrer text,
    out_of_pocket text
);


